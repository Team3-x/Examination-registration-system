<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Ks</display-name>
	<servlet>
		<servlet-name>stulogin</servlet-name>
		<servlet-class>com.zhonghuan.sx.controllor.StuloginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>stulogin</servlet-name>
		<url-pattern>/stulogin</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>memberlogin</servlet-name>
		<servlet-class>com.zhonghuan.sx.controllor.MemberLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>memberlogin</servlet-name>
		<url-pattern>/memberlogin</url-pattern>
	</servlet-mapping>
	<error-page>
		<error-code>404</error-code>
		<location>/404.jsp</location>
	</error-page>
</web-app>